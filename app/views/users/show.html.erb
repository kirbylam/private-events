<h1><%= @user.username %>'s Profile</h1>

<h2>Events Created</h2>
<ul>
  <% @created_events.each do |event| %>
    <li><%= link_to event.name, event %></li>
  <% end %>
</ul>

<h2>Events Attending</h2>
<ul>
  <% @events_attending.each do |event| %>
    <li><%= link_to event.name, event %></li>
  <% end %>
</ul>
